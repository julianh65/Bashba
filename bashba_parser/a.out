; ModuleID = 'MicroC'
source_filename = "MicroC"

@str = global i0 0
@fmt = private unnamed_addr constant [4 x i8] c"%d\0A\00", align 1
@str.1 = private unnamed_addr constant [17 x i8] c"hello world, 15!\00", align 1

declare i32 @printf(i8*, ...)

define i32 @main() {
entry:
  store i8* getelementptr inbounds ([17 x i8], [17 x i8]* @str.1, i32 0, i32 0), i0* @str, align 8
  ret i32 0
}
